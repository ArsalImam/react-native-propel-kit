{
  "name": "@mgcrea/react-native-date-picker",
  "version": "0.8.6",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:source && yarn build:typings",
    "build:source": "NODE_ENV=production babel --verbose --source-maps --extensions .js,.ts,.tsx --out-dir lib/ --delete-dir-on-start --ignore **/__tests__ src/",
    "build:typings": "NODE_ENV=production tsc --declaration",
    "typecheck": "tsc --pretty --noEmit",
    "test": "CI=true jest --runInBand --colors"
  },
  "dependencies": {
    "@mgcrea/react-native-button": "0.8.6",
    "@mgcrea/react-native-modal-dialog": "0.8.6"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-native": ">=0.59"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@storybook/react-native": "^5.2.8",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.15",
    "@types/react": "^16.9.16",
    "@types/react-native": "^0.60.24",
    "@types/react-test-renderer": "^16.9.1",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "react": "^16.9.0",
    "react-native": "^0.61.0",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "^16.12.0",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.3"
  }
}
