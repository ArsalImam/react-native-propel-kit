{
  "name": "@mgcrea/react-native-date-picker",
  "version": "0.8.7",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:source && yarn build:typings",
    "build:source": "NODE_ENV=production babel --verbose --source-maps --extensions .js,.ts,.tsx --out-dir lib/ --delete-dir-on-start --ignore **/__tests__ src/",
    "build:typings": "NODE_ENV=production tsc --declaration",
    "typecheck": "tsc --pretty --noEmit",
    "test": "CI=true jest --runInBand --colors"
  },
  "dependencies": {
    "@mgcrea/react-native-button": "0.8.7",
    "@mgcrea/react-native-modal-dialog": "0.8.7"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-native": ">=0.59"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@storybook/react-native": "^5.3.18",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.2",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.4.0",
    "jest": "^25.4.0",
    "react": "^16.9.0",
    "react-native": "^0.61.0",
    "react-native-testing-library": "^1.13.0",
    "react-test-renderer": "^16.13.1",
    "styled-components": "^5.1.0",
    "typescript": "^3.8.3"
  }
}
